<div class="container">
  <div class="title-container">
    <h2>Controle de Acesso</h2>
  </div>
  <div class="tbl-header">
    <table>
      <thead>
        <!-- <th>id do Usu√°rio</th> -->
        <th>Linhas</th>
        <th>UUID</th>
        <th>Pagina</th>
        <th>Data Acesso</th>
      </thead>
    </table>
  </div>
    <div class="tbl-content">
      <table>
        <tbody>
          <% @acessos.each_with_index do |acesso,index| %>
            <tr>
              <td><%= index+1 %></td>
              <th scope="row"><%= acesso.uuid %></th>
              <td><%= acesso.page %></td>
              <td><%= acesso.sysdate.strftime("%d/%m/%Y - %H:%M") %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
</div>
